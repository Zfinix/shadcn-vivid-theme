  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#434343d6"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#b7ff9d"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#f43f5e"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#0b9bf5"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Object",
      "scope": [
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#cae0ff"
      }
    },
    {
      "name": "Class Name",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#f4ff7c"
      }
    },
    {
      "name": "Function Name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#fc7f92"
      }
    },
    {
      "name": "Function Argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#7dabff"
      }
    },
    {
      "name": "Tag Name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#f0d088"
      }
    },
    {
      "name": "Library Function",
      "scope": "support.function",
      "settings": {
        "foreground": "#6ee1fb"
      }
    },
    {
      "name": "Library Constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#f59e0b"
      }
    },
    {
      "name": "Library Class/Type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#f4ff7c"
      }
    },
    {
      "name": "Library Variable",
      "scope": "support.other.variable",
      "settings": {
        "foreground": "#b9545c"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#ef4444",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#e2e8f0"
      }
    },
    {
      "name": "CSS Property Values",
      "scope": "meta.property-value.css",
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "name": "JSX/TSX Tags",
      "scope": [
        "meta.tag.js entity.name.tag",
        "meta.tag.tsx entity.name.tag"
      ],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "JSX/TSX Attributes",
      "scope": [
        "meta.tag.js entity.other.attribute-name",
        "meta.tag.tsx entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#9a9ac1"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#85ac71"
      }
    },
    {
      "name": "Markdown Links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#4ade80"
      }
    },
    {
      "name": "Markdown Code",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#489765"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#a6a6a6d8"
      }
    },
    {
      "name": "TypeScript Decorators",
      "scope": [
        "meta.decorator entity.name.function",
        "meta.decorator punctuation.decorator"
      ],
      "settings": {
        "foreground": "#ff9195"
      }
    },
    {
      "name": "CSS Classes",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#f4ff7c"
      }
    },
    {
      "name": "CSS IDs",
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "CSS Units",
      "scope": "keyword.other.unit.css",
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "HTML Entities",
      "scope": "constant.character.entity.html",
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "HTML Doctype",
      "scope": "meta.tag.sgml.doctype",
      "settings": {
        "foreground": "#6b7280"
      }
    },
    {
      "name": "Python Decorators",
      "scope": [
        "meta.function.decorator.python",
        "entity.name.function.decorator.python"
      ],
      "settings": {
        "foreground": "#fda3a6"
      }
    },
    {
      "name": "GraphQL Field Names",
      "scope": "meta.selectionset.graphql variable",
      "settings": {
        "foreground": "#afb3b8"
      }
    },
    {
      "name": "YAML Keys",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Import/Export Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#e23e3e"
      }
    },
    {
      "name": "Control flow keywords",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#ffffff80"
      }
    },
    {
      "name": "File paths",
      "scope": [
        "string.quoted.double.filename",
        "string.quoted.single.filename",
        "string.unquoted.filename",
        "constant.other.file-path"
      ],
      "settings": {
        "foreground": "#858585"
      }
    },
    {
      "name": "Import/require file paths",
      "scope": [
        "string.quoted.module",
        "string.unquoted.module",
        "meta.import string"
      ],
      "settings": {
        "foreground": "#4f5255"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Dart Property Access",
      "scope": [
        "variable.other.dart",
        "variable.other.property.dart"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
  ]
}